DECLARE PLUGIN "coq-ol.plugin"


{

open Ltac_plugin
(*open Pp*)
open Stdarg

}


TACTIC EXTEND OLGet
| [ "olget" constr(e) ident(h) ] -> 
  { 
   Ce_api.ol_normal e h
  }
END

TACTIC EXTEND OLCert
| [ "olcert" constr(e) ident(h) ] -> 
  { 
   Ce_api.ol_cert_tactic e h
  }
END

TACTIC EXTEND DestructAtom
| [ "destruct_atom" ] -> 
  { 
   Ce_api.destruct_atom
  }
END